{
  "name": "airbnb",
  "version": "1.0.0",
  "description": "airbnb clone project",
  "main": "src/server.js",
  "scripts": {
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage",
    "dev": "nodemon ./src/server.js",
    "start": "node ./src/server.js",
    "eslint-check": "eslint --ignore-path .gitignore .",
    "eslint": "eslint --fix --ignore-path .gitignore .",
    "lint": "yarn eslint",
    "prettier": "prettier --write \"./**/*.{md,json,html,css,js,yml}\"",
    "prettier-check": "prettier --check \"./**/*.{md,json,html,css,js,yml}\""
  },
  "author": "Jun Song",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.js": [
      "yarn prettier",
      "yarn eslint",
      "yarn test"
    ]
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "client-sessions": "^0.8.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.1.0",
    "express-pino-logger": "^5.0.0",
    "moment": "^2.29.1",
    "mongoose": "^5.11.4",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.14",
    "pino": "^6.7.0",
    "pino-pretty": "^4.3.0"
  },
  "devDependencies": {
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.2.1"
  }
}
